{
  "expo": {
    "name": "oflo",
    "slug": "oflo",
    "version": "1.0.0",
    "scheme": "oflo",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",

    "icon": "./src/assets/images/icon.png",

    "newArchEnabled": true,
    "experiments": { "typedRoutes": true },

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iliarocks.oflo"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/roboto-mono/500Medium/RobotoMono_500Medium.ttf",
            "node_modules/@expo-google-fonts/roboto-mono/700Bold/RobotoMono_700Bold.ttf"
          ]
        }
      ]
    ],

    "extra": {
      "router": {},
      "eas": { "projectId": "0ec6069c-c068-4a8d-8053-2c8c28449c72" }
    }
  }
}
